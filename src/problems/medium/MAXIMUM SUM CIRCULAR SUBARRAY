#include<iostream>
using namespace std;

int main(){
  int n;
  cin >> n;
  int arr[n];
  for(int i=0;i<n;i++){
    cin >> arr[i];
  }  
  int warpsum;
  int nowrapsum;
  
  nowrapsum=kadane(arr,n);
  int total_sum=0;
  
  for(int i=0;i<n;i++){
    total_sum+=arr[i];
    arr[i]=-arr[i];
  }
  wrapsum=total_sum + kadane(arr,n);
  cout << max(nowrapsum,wrapsum) << endl;
    return 0;
 }
   
  int kadane(int arr[],int n){
    int currsum=0;
    int maxsum=INT_MIN;
    
    for(int i=0;i<n;i++){
        currsum+=arr[i];
        if(currsum<0){
          currsum=0;
        }
        maxsum=max(currsum,maxsum);
        return maxsum;
    }
